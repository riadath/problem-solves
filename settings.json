        "cpp": "cd $dir && g++  -Wextra -O2 -D_GLIBCXX_DEBUG -D_GLIBCXX_DEBUG_PEDANTIC -D_FORTIFY_SOURCE=2 -DIHAVETOPEE $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
